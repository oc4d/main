<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>
		<auto_play>
			<defaults symbol="circular"/>
			<tweenIn x="850" y="45" width="30" height="30" tint="0x000000" alpha="0.5"/>
			<tweenOver alpha="1"/>
		</auto_play>
    	<prev_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</prev_button>
		
    	<prev_symbol>
			<tweenOver tint="0x000000" />			
		</prev_symbol>
		
    	<next_button>
			<defaults round_corners="5,5,5,5"/>			
			<tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</next_button>
		
    	<next_symbol>
			<tweenOver tint="0x000000" />
		</next_symbol>	
			
		<description>
		<defaults 
			round_corners="10,10,10,10" 
			
			heading_font="Lucida Sans Unicode" 
			heading_text_size="22" 
			heading_text_color="0xffffff" 
			heading_text_margin="15,15,0,15" 
			
			paragraph_font="Lucida Sans Unicode" 
			paragraph_text_size="12" 
			paragraph_text_color="0xffffff" 
			paragraph_text_margin="5,15,15,15"
		/>
		
		<tweenIn x="100" y="190" width="740" height="100" alpha="0.45"/>
		<tweenOver alpha="0.8"/>
		</description>	
			
	</settings>    

	<slides>
        <slide>
            <url>wp-content/uploads/2009/12/slide_1.jpg</url>
        </slide>
		<transition num="6" slicing="vertical" direction="down" shader="flat" delay="0.05"/>
		<slide>
            <url>wp-content/uploads/2009/12/slide_2.jpg</url>

        </slide>
		<transition num="1" direction="left" z_multiplier="3" delay="0.1"/>
		<slide>
            <url>wp-content/uploads/2009/12/slide_3.jpg</url>
        </slide>
		<transition num="3" direction="down" z_multiplier="2.1" delay="0.09" shader="phong" />
				
	</slides>
</cu3er>

